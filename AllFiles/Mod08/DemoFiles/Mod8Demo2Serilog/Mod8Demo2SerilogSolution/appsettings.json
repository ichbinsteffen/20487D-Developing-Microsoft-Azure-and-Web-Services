{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

    "Serilog": {
      "MinimumLevel": "Information",
      "WriteTo": [
        {
          "Name": "MSSqlServer",
          "Args": {
            "connectionString": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=SerilogDB;Integrated Security=True;Pooling=False",
            "tableName": "Logs",
            "autoCreateSqlTable": true
          }
        }
      ]
    }
  },

  "AllowedHosts": "*"
}
